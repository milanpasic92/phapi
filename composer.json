{
  "name": "milanpasic92/phapi",
  "type": "framework",
  "version": "1.0.1",
  "description": "Phalcon API skeleton app",
  "homepage": "https://github.com/milanpasic92/phapi",
  "license": "mit",
  "keywords": ["php","phalcon","api"],
  "authors": [
    {
      "name": "Milan Pasic",
      "email": "milanpasic92@gmail.com"
    }
  ],
  "require": {
    "php": "~7.4",
    "ext-intl": "*",
    "ext-json": "*",
    "monolog/monolog": "~2.2",
    "firebase/php-jwt": "~5.2",
    "milanpasic92/phalcon-repositories": "dev-master",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phalcon/ide-stubs": "~4.1"
  },
  "autoload": {
    "psr-4": {
